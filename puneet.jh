







entity Employee {
	/**
	* The firstname attribute.
	*/
	firstName String,
	lastName String,
	email String,
	phoneNumber String,
	hireDate Instant,
	salary Long,
	commissionPct Long
}

entity Job {
	jobTitle String,
	minSalary Long,
	maxSalary Long
}



enum Language {
    FRENCH, ENGLISH, SPANISH
}







dto * with mapstruct

// Set service options to all except few
service all with serviceImpl except Employee
// Set an angular suffix
angularSuffix * with mySuffix
